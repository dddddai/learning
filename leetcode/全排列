class Solution(object):
    def permute(self, nums):
        """
        :type nums: List[int]
        :rtype: List[List[int]]
        """
        # from itertools import permutations
        # return list(permutations(nums))
        if len(nums)<=1:
            return [nums]
        result=[]
        for i in xrange(len(nums)):
            for p in self.permute(nums[:i]+nums[i+1:]):
                result.append([nums[i]]+p)
        return result
