class Solution(object):
    cnt=0
    int_max=2**32-1
    def kthSmallest(self, root, k):
        """
        :type root: TreeNode
        :type k: int
        :rtype: int
        """
        if root is None:
            return self.int_max
        result=self.kthSmallest(root.left,k)
        if result!=self.int_max:
            return result
        self.cnt+=1
        if self.cnt==k:
            return root.val
        return self.kthSmallest(root.right,k)

